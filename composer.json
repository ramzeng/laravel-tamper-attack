{
    "name": "ramzeng/laravel-tamper-attack",
    "description": "laravel middleware to prevent tamper attacks.",
    "license": "MIT",
    "authors": [
        {
            "name": "ramzeng",
            "email": "srizym@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Ramzeng\\LaravelTamperAttack\\": "src",
            "Ramzeng\\LaravelTamperAttack\\Tests\\": "tests"
        }
    },
    "scripts": {
        "pint": "vendor/bin/pint",
        "test": "vendor/bin/phpunit --colors=always"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "phpunit/phpunit": "^10.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ramzeng\\LaravelTamperAttack\\ServiceProvider"
            ]
        }
    }
}